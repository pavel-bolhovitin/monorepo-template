{
    "extends": "./base.json",
    "compilerOptions": {
        "incremental": true,
        "noEmit": true,
        "plugins": [
            {
                "name": "next"
            }
        ]
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "next-env.d.ts",
        ".next/types/**/*.ts",
        ".next/dev/types/**/*.ts",
        "**/*.mts"
    ]
}